body
  header.headerM#inicio
    section.headerM-slider
      .galery
        +galeria([
              ['uno', 'true'],
              ['dos'],
              ['tres'],
              ['cuatro'],
              ],
              [['slide01.jpg', 'Un nuevo array'],
              ['slide02.jpg', 'prueba perfecta'],
              ['slide05.jpg', 'buena validacion'],
              ['slide04.jpg', 'buena validacion']
              ])
      .imgSlider
        //- img(src="./images/slide01.jpg" alt="imagen publicitaria" srcset="./images/slide250.jpg 50w")
        //- picture
        //-   source(media="(max-width:400px)", srcset="./images/slide250.jpg")
        //-   source(media="(max-width:320px)", srcset="./images/slide250.jpg")
        img(src="./images/slide250.jpg", alt="imagen publicitaria" height="250")
    .headerM-project
      h1.titleInfo APARTAMENTOS EN EDIFICIO INTELIGENTE #[span BOGOTÁ]
      .headerM-project--infoBox
        //- .boxHover
        .iBox.b01
            .iBox-container
              .icon
              H3.text
                a(href="tipoa.html") "APARTAMENTO" Tipo A
        //- .boxHover
        .iBox.b02
            .iBox-container
              .icon
              h3.text
                a(href="tipob.html") "APARTAMENTO" Tipo B

  main.mainContainer#proyecto
    section.mainContainer-infoBox
        .infoBoxWrap
          .ibImage.iBG01
            //- img(src="./images/c03.png", alt="prueba" height="230")
          .content
            h3.titleInfoBox CÓMODAS HABITACIONES
        .infoBoxWrap
          .ibImage.iBG02
            //- img(src="./images/c01.png", alt="prueba" height="230")
          .content
            h3.titleInfoBox AMPLIOS ESPACIOS
        .infoBoxWrap
          .ibImage.iBG03
            //- img(src="./images/c04.png", alt="prueba" height="230")
          .content
            h3.titleInfoBox ELEMENTOS ÚNICOS
    .mainContainer-descrip
      h3.titleDescrip ESPACIOS SOSTENIBLES PARA SU BIENESTAR Y CONFORT
      .descripciones
        ul.listDescrip.u01
          li #[i.fa.fa-chevron-circle-right] Terraza comunal BBQ
          li #[i.fa.fa-chevron-circle-right] Videovigilancia
          li #[i.fa.fa-chevron-circle-right] Bicicleteros
          li #[i.fa.fa-chevron-circle-right] Salón Comunal
        ul.listDescrip.u02
          li #[i.fa.fa-chevron-circle-right] Depósitos
          li #[i.fa.fa-chevron-circle-right] Ascensor
          li #[i.fa.fa-chevron-circle-right] Excelente ubicación
    .mainContainer-video
      .mainContainer-titulo
        h2 Recorrido Virtual
      .mainContainer-recurso
        .video
          <iframe width="560" height="315" src="https://www.youtube.com/embed/21hmTVEnl3w?rel=0" frameborder="0" allow="autoplay; encrypted-media" allowfullscreen></iframe>
    .mainContainer-info
      .patrocinadores
        .imPat.i01
            img(src="./images/vigiladoF.png" alt="fiduciria" height="170px")
        .imPat.i02
          h4 Diseño
          img(src="./images/diseño.png" alt="fiduciria" height="40px")
        .imPat.i03
          h4 Construye
          img(src="./images/construye.png" alt="fiduciria" height="40px")
        .imPat.i04
          h4 Promotor
          img(src="./images/promotor.png" alt="fiduciria" height="40px")
  section.location#ubicacion
    .location-map
      <iframe src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d7953.144034337733!2d-74.0865302!3d4.6701265!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x8e3f9b065f8400af%3A0xa0a9e6854d72cc77!2sCl.+67+%2363-65%2C+Bogot%C3%A1!5e0!3m2!1ses!2sco!4v1517985816425" width="600" height="450" frameborder="0" style="border:0" allowfullscreen></iframe>
    .location-mapText
      .locationWrap
        h3 UBICACIÓN
        .location-address
          .location-icon
            i.fa.fa-map-marker
          .location-text
            p CALLE 67A # 63-65
            p BARRIO JJ VARGAS
            p BOGOTÁ D.C.
  section.contactContainer#contacto
    .contactContainer-address
      .contactWrap
        h3.contactWrap-title CONTACTO:
        .contactWrap-phones
          .contactWrap-icon
            i.fa.fa-phone
          .contactWrap-text
            p (+57) 300 790 6372
            p (+57) 313 247 4228
            p (+57) 313 817 9522
      .url
        p WWW.REFUGIODELZORZAL.CO
    .contactContainer-form
      +form()
      block forms
  footer.piePagina
    .piePagina-copyright
      p.t1 copyright &#169 2018 // Todos los derechos reservados a #{empresa}
      p.t3 Desarrollado por &nbsp
        a(href="http://www.codigocreativo.com.co" target="blanck" rel="nofollow") #[span CreativeCode]

      p.t4 Villavicencio - Meta
    .piePagina-logoFinal
      img.imgFinal(src="./images/logoCompleto.png" width="20%")
